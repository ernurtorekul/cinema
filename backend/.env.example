# Mock Mode (set to true to test without Supabase database)
MOCK_MODE=false

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_anon_key
SUPABASE_SERVICE_KEY=your_service_role_key

# Redis
REDIS_URL=redis://localhost:6379/0

# OpenAI API Keys (keys start with sk-proj- or sk-)
OPENAI_SCENARIO_AGENT_API_KEY=sk-proj-your_openai_api_key_here

# Claude API Keys (keys start with sk-ant-)
CLAUDE_PROMPT_AGENT_API_KEY=sk-ant-your_claude_api_key_here

# Gemini AI API Keys (keys start with AIzaSy...)
# Get your keys at: https://aistudio.google.com/app/apikey
GEMINI_CHARACTER_AGENT_API_KEY=AIzaSyYourGeminiApiKeyHere
GEMINI_SOUND_DESIGN_AGENT_API_KEY=AIzaSyYourGeminiApiKeyHere
GEMINI_SOURCE_AGENT_API_KEY=AIzaSyYourGeminiApiKeyHere

# Audio APIs (Optional)
ELEVENLABS_API_KEY=your_elevenlabs_key
SUNO_API_KEY=your_suno_key
UDIO_API_KEY=your_udio_key

# CORS
FRONTEND_URL=http://localhost:3000
